    "build:stage": "ng build --watch=false --base-href /xrmv2_stage/ --configuration staging --output-path=dist/staging --progress=true",
    "build:prod": "ng build --watch=false --base-href /qa_ui/ --configuration production --output-path=dist/prod --progress=true",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
