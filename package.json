    "prebuild": "npm run increment-version",
    "build": "ng build",
    "build:dev": "ng build --watch=false --base-href /dev_ui/ --configuration devtest --output-path=dist/dev --progress=true",
    "build:local": "ng build --watch=false --base-href /dev_ui/ --configuration local --output-path=dist/local --progress=true",
